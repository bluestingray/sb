<odoo>
    <!--Create Top Menu-->
    <menuitem name="Top Menu" id="custom_top_menu"/>
    <menuitem name="Sub Child Menu" id="custom_sub_menu" parent="custom_top_menu"/>
    <!-- Define custom action -->
    <record id="custom_action" model="ir.actions.act_window">
        <field name="name">Child Menu
        </field>
        <field name="res_model">mmm</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">Click here to create a new item</p>
        </field>
    </record>

    <menuitem name="Child Menu" id="custom_child_menu" parent="custom_sub_menu" action="custom_action"/>

</odoo>
